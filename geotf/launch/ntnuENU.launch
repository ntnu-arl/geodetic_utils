<launch>
    <rosparam param="/geotf" >
        Frames:
            ENU_NTNU:
                Type: ENUOrigin
                LonOrigin:  10.399086
                LatOrigin:  63.418578
                AltOrigin:  80.0

            GPS:
                Type: GCSCode
                Code: WGS84

            UTM:
                Type: UTM
                Zone: 32
                Hemisphere: N

            <!-- Also known as EUREF89 in Norway -->
            ETRS89: 
                Type: EPSGCode
                Code: 5972

        TF_Mapping:
            GEO_TF: ENU_NTNU
            TF: enu
    </rosparam>

    <node pkg="tf" type="static_transform_publisher" name="enubodybroadcaster" args="1.25 1.25 0 0 0 0 1 enu body 100" />
    <node name="ntnu_node" type="ntnu_node" pkg="geotf" output="screen" />
</launch>